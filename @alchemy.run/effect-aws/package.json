  "homepage": "https://alchemy.run",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-goodwin/alchemy.git",
    "directory": "alchemy"
  },
  "type": "module",
  "module": "./lib/index.js",
  "sideEffects": false,
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./lambda": {
      "bun": "./src/lambda/index.ts",
      "import": "./lib/lambda/index.js",
      "types": "./lib/lambda/index.d.ts"
    },
    "./sqs": {
      "bun": "./src/sqs/index.ts",
      "import": "./lib/sqs/index.js",
      "types": "./lib/sqs/index.d.ts"
    },
    "./sts": {
      "bun": "./src/sts.ts",
      "import": "./lib/sts.js",
      "types": "./lib/sts.d.ts"
    },
    "./iam": {
      "bun": "./src/iam.ts",
      "import": "./lib/iam.js",
      "types": "./lib/iam.d.ts"
    },
    "./credentials": {
      "bun": "./src/credentials.ts",
      "import": "./lib/credentials.js",
      "types": "./lib/credentials.d.ts"
    }
  },
  "peerDependencies": {
    "@alchemy.run/effect": "workspace:*",
    "@effect/platform-node": "*",
    "@effect/platform": "*"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.0.0",
    "@clack/prompts": "^0.11.0",
