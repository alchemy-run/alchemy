    "@types/aws-lambda": "^8.10.152",
    "aws4fetch": "^1.0.20",
    "effect": "^3.17.14",
    "fast-xml-parser": "^5.2.5",
    "itty-aws": "^0.3.6",
    "picocolors": "^1.1.1",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@effect/platform-node": "^0.97.1",
    "@effect/platform": "^0.91.1",
    "@types/bun": "latest",
    "@types/node": "latest",
    "ink": "^6.3.1"
  }
}
