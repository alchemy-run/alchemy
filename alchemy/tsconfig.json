{
  "extends": "../tsconfig.base.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": null,
    "types": ["@cloudflare/workers-types", "@types/node"],
    "rootDir": "./src",
    "outDir": "./lib",
    "noEmit": false,
    "esModuleInterop": true,
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "target": "ESNext"
  }
}
