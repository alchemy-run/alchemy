{
  "name": "alchemy-mono",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "module": "./lib/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "docs:dev": "bun run --filter alchemy-web dev",
    "docs:gen": "bun run --filter alchemy-web gen",
    "bootstrap": "bun run --env-file .env ./alchemy.config.ts --verbose",
    "build": "tsc -b",
    "check": "tsc -b ./tsconfig.json",
    "publish:npm": "bun run --filter alchemy publish:npm"
  },
  "workspaces": ["alchemy", "alchemy-web", "examples/*"],
  "devDependencies": {
    "@biomejs/biome": "~1.8.0",
    "@types/bun": "latest",
    "@types/node": "latest",
    "typescript": "latest",
    "yaml": "^2.7.1"
  }
}
