{
  "name": "@redwoodjs/starter-drizzle",
  "version": "1.0.0",
  "description": "A RedwoodSDK starter for projects with a database using Drizzle",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "bun run build && bun run vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "bun run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && bun run clean && prisma generate && RWSDK_DEPLOY=1 bun run build && wrangler deploy",
    "format": "prettier --write ./src",
    "migrate:new": "drizzle-kit generate",
    "migrate:dev": "wrangler d1 migrations apply DB --local",
    "seed": "bun run worker:run ./src/db/seed.ts",
    "check": "bun run types",
    "types": "bun run tsc"
  },
  "dependencies": {
    "@redwoodjs/sdk": "0.0.51",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.3",
    "react": "19.0.0-rc-f2df5694-20240916",
    "react-dom": "19.0.0-rc-f2df5694-20240916",
    "react-server-dom-webpack": "19.0.0-rc-f2df5694-20240916"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "alchemy": "workspace:*",
    "drizzle-kit": "^0.30.4",
    "pnpm": "^10.9.0",
    "tsx": "^4.19.2",
    "vite": "^6.1.1",
    "wrangler": "^4.4.0"
  }
}
